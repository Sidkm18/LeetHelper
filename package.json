{
    "name": "leet-help",
    "displayName": "LeetHelp",
    "publisher": "leethelp",
    "icon": "resources/icon.png",
    "description": "Solve LeetCode problems within VS Code",
    "repository": {
        "type": "git",
        "url": "https://github.com/leethelp/leethelp"
    },
    "version": "1.0.2",
    "engines": {
        "vscode": "^1.80.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onView:leetcode-problems",
        "onCommand:leethelp.signIn",
        "onCommand:leethelp.signOut",
        "onCommand:leethelp.authStatus",
        "onCommand:leethelp.dailyQuestion",
        "onCommand:leethelp.runTest",
        "onCommand:leethelp.runCustomTest",
        "onCommand:leethelp.submit",
        "onCommand:leethelp.searchProblem",
        "onCommand:leethelp.refresh",
        "onCommand:leethelp.openProblem",
        "onCommand:leethelp.verifyCookie"
    ],
    "main": "./dist/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "leethelp.signIn",
                "title": "LeetHelp: Sign In"
            },
            {
                "command": "leethelp.signOut",
                "title": "LeetHelp: Sign Out"
            },
            {
                "command": "leethelp.authStatus",
                "title": "LeetHelp: Auth Status"
            },
            {
                "command": "leethelp.dailyQuestion",
                "title": "LeetHelp: Open Daily Question",
                "icon": "$(calendar)"
            },
            {
                "command": "leethelp.runTest",
                "title": "LeetHelp: Run Test",
                "icon": "$(debug-start)"
            },
            {
                "command": "leethelp.runCustomTest",
                "title": "LeetHelp: Run Custom Test",
                "icon": "$(beaker)"
            },
            {
                "command": "leethelp.submit",
                "title": "LeetHelp: Submit",
                "icon": "$(cloud-upload)"
            },
            {
                "command": "leethelp.searchProblem",
                "title": "LeetHelp: Search Problem",
                "icon": "$(search)"
            },
            {
                "command": "leethelp.refresh",
                "title": "LeetHelp: Refresh Problems",
                "icon": "$(refresh)"
            },
            {
                "command": "leethelp.verifyCookie",
                "title": "LeetHelp: Verify Cookie"
            }
        ],
        "menus": {
            "editor/title": [
                {
                    "command": "leethelp.runTest",
                    "group": "navigation@1"
                },
                {
                    "command": "leethelp.runCustomTest",
                    "group": "navigation@2"
                },
                {
                    "command": "leethelp.submit",
                    "group": "navigation@3"
                }
            ],
            "view/title": [
                {
                    "command": "leethelp.dailyQuestion",
                    "when": "view == leetcode-problems",
                    "group": "navigation@1"
                },
                {
                    "command": "leethelp.searchProblem",
                    "when": "view == leetcode-problems",
                    "group": "navigation@2"
                },
                {
                    "command": "leethelp.refresh",
                    "when": "view == leetcode-problems",
                    "group": "navigation@3"
                }
            ]
        },
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "leetcode-view-container",
                    "title": "LeetCode",
                    "icon": "resources/icon.svg"
                }
            ]
        },
        "views": {
            "leetcode-view-container": [
                {
                    "id": "leetcode-problems",
                    "name": "Problems",
                    "icon": "resources/icon.svg"
                }
            ]
        },
        "configuration": {
            "title": "LeetHelp",
            "properties": {}
        },
        "keybindings": [
            {
                "command": "leethelp.runTest",
                "key": "ctrl+alt+r",
                "mac": "cmd+alt+r",
                "when": "editorTextFocus"
            },
            {
                "command": "leethelp.runCustomTest",
                "key": "ctrl+alt+c",
                "mac": "cmd+alt+c",
                "when": "editorTextFocus"
            },
            {
                "command": "leethelp.submit",
                "key": "ctrl+alt+s",
                "mac": "cmd+alt+s",
                "when": "editorTextFocus"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run package",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "package": "tsc -p ./",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/node": "^18.14.6",
        "@types/vscode": "^1.80.0",
        "ts-loader": "^9.4.3",
        "typescript": "^5.1.3",
        "webpack": "^5.85.0",
        "webpack-cli": "^5.1.1"
    },
    "dependencies": {
        "axios": "^1.6.0",
        "isomorphic-dompurify": "^2.34.0"
    }
}