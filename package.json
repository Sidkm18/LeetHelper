{
    "name": "leet-help",
    "displayName": "LeetHelp",
    "publisher": "leethelp",
    "icon": "resources/icon.png",
    "description": "Solve LeetCode problems within VS Code",
    "repository": {
        "type": "git",
        "url": "https://github.com/leethelp/leethelp"
    },
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.80.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [],
    "main": "./dist/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "leethelp.signIn",
                "title": "LeetCode: Sign In"
            },
            {
                "command": "leethelp.signOut",
                "title": "LeetCode: Sign Out"
            },
            {
                "command": "leethelp.runTest",
                "title": "LeetCode: Run Test",
                "icon": "$(play)"
            },
            {
                "command": "leethelp.submit",
                "title": "LeetCode: Submit",
                "icon": "$(cloud-upload)"
            },
            {
                "command": "leethelp.searchProblem",
                "title": "Search Problem",
                "category": "LeetHelp",
                "icon": "$(search)"
            },
            {
                "command": "leethelp.refresh",
                "title": "Refresh Problems",
                "category": "LeetHelp",
                "icon": "$(refresh)"
            }
        ],
        "menus": {
            "editor/title": [
                {
                    "command": "leethelp.runTest",
                    "group": "navigation"
                },
                {
                    "command": "leethelp.submit",
                    "group": "navigation"
                }
            ],
            "view/title": [
                {
                    "command": "leethelp.searchProblem",
                    "when": "view == leetcode-problems",
                    "group": "navigation"
                },
                {
                    "command": "leethelp.refresh",
                    "when": "view == leetcode-problems",
                    "group": "navigation"
                }
            ]
        },
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "leetcode-view-container",
                    "title": "LeetCode",
                    "icon": "resources/icon.png"
                }
            ]
        },
        "views": {
            "leetcode-view-container": [
                {
                    "id": "leetcode-problems",
                    "name": "Problems"
                }
            ]
        },
        "configuration": {
            "title": "LeetHelp",
            "properties": {}
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run package",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "package": "tsc -p ./",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/node": "^18.14.6",
        "@types/vscode": "^1.80.0",
        "ts-loader": "^9.4.3",
        "typescript": "^5.1.3",
        "webpack": "^5.85.0",
        "webpack-cli": "^5.1.1"
    },
    "dependencies": {
        "axios": "^1.6.0",
        "isomorphic-dompurify": "^2.34.0"
    }
}